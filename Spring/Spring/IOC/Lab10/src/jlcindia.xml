<?xml version="1.0" encoding="UTF-8"?>
<beans 
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="ao1" class="com.jlcindia.spring.A">
		<property name="a" value="99" />
		<property name="msg" value="Hello Guys" />
	</bean>
	
	<bean id="ao2" class="com.jlcindia.spring.A">
		<property name="a" value="11" />
		<property name="msg" value="Hai Guys" />
	</bean>

	<bean id="bo" class="com.jlcindia.spring.B">
		<constructor-arg value="88" />
		<constructor-arg value="Guys" />
	</bean>
	
	<bean id="bobj" class="com.jlcindia.spring.B">
		<constructor-arg value="88" />
		<constructor-arg value="Guys" />
	</bean>

	<bean id="hello" class="com.jlcindia.spring.Hello" autowire="constructor">
	</bean>


</beans>