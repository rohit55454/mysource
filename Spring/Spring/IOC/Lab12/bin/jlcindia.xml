<?xml version="1.0" encoding="UTF-8"?>
<beans 
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:jlc="http://www.springframework.org/schema/p"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	
	<bean id="aobj" class="com.jlcindia.spring.A" jlc:a="10" jlc:msg="AAA" />

	<bean id="bobj" class="com.jlcindia.spring.B" jlc:b="20" jlc:str="BBB" />
	
	<bean id="hello" class="com.jlcindia.spring.Hello" jlc:aobj-ref="aobj" jlc:bobj-ref="bobj" />

</beans>