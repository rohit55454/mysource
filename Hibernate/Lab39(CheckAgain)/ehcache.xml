<ehcache>
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="100"
				  eternal = "false"
				  timeToIdleSeconds="120"
				  timeToLiveSeconds="120"
				  overFlowToDisk="true" />
	
	<cache name="jlcindiacache"
				  maxElementsInMemory="100"
				  eternal = "false"
				  timeToIdleSeconds="3000"
				  timeToLiveSeconds="6000"
				  overFlowToDisk="true" />

</ehcache>